{
    "version":2,
    "builds":
    [
        {"src": "./index.js", "use": "@vercel/node"},
        {"src": "./**/*.js","use": "@vercel/node"},
        {"src": "./**/*","use": "@vercel/static"}
    ],
    "routes":[
    {"src": "/(.*)", "dest": "/index.js"},
    { "src": "/Scripts/custom(.*)", "dest": "/Scripts/custom/$1" },
    { "src": "/Content(.*)", "dest": "/Content/$1" }
],
    "installCommand": "npm install"
}