{
    "version":2,
    "builds":
    [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
          },
          {
            "src": "public/**/*",
            "use": "@vercel/static"
          },
        {"src": "./**/*.js","use": "@vercel/node"}

    ],
    "routes":[
    {"src": "/api", "dest": "/index.js"},
    {
      "src": "/(.*)",
      "dest": "./index.html"
    }
],
    "installCommand": "npm install"
}